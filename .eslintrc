---
"extends":
  - "formidable/configurations/es6-react"

"rules":
  "indent": ["warn", 4]
  "max-len": 0
  "no-magic-numbers": 0
  "react/jsx-indent-props": ["warn", 4]
  "react/prefer-es6-class": 0
  "react/no-multi-comp": 0

"env":
  "browser": true,
  "node": true
"globals":
  "afterEach": true,
  "describe": true,
  "expect": true,
  "it": true,
  "jest": true,
  "test": true
